{% extends 'templates/base.twig' %}

{% block container %}
    <h1>List of roles for {{ guild_name }}</h1>
<table class="striped centered">
    <thead>
        <tr>
            <th>Id</th>
            <th>name</th>
        </tr>
    </thead>
    <tbody>
        {% for role in roles %}
            <tr>
                <td>{{ role.id }}</td>
                <td>{{ role.name }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}
